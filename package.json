{
  "name": "overmind-router",
  "version": "2.0.0",
  "description": "State machine-driven router for Overmind applications",
  "keywords": [
    "overmind",
    "router",
    "state machine",
    "react"
  ],
  "author": "Henri Hulski <henri@sancode.it>",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/henri-hulski/overmind-router.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "scripts": {
    "build": "tsdown",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "prettier . --write --list-different && npm run lint -- --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "overmind": "^28.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/node": "^24.9.2",
    "@vitest/coverage-v8": "^4.0.6",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.4.0",
    "jsdom": "^27.1.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.12",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.6"
  }
}
