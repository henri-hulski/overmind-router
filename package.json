{
  "name": "overmind-router",
  "version": "1.0.0",
  "description": "State machine-driven router for Overmind applications",
  "keywords": [
    "overmind",
    "router",
    "state machine",
    "react"
  ],
  "scripts": {
    "build": "vite build",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "prettier . --write --list-different && npm run lint -- --fix",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "start": "vite serve --force",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "overmind": "^28.1.0-1750163997957",
    "overmind-react": "^29.1.0-1750163997957",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.32",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
